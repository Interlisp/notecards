(DEFINEQ

(NC.MakeLogoWindow
  (LAMBDA NIL                                                (* rht%: "11-Nov-87 11:55")
    (SETQ NC.LogoWindow (CREATEW (CREATEREGION 0 0 (BITMAPWIDTH LOGOBM)
                                        (BITMAPHEIGHT LOGOBM))
                               NIL NIL T))
    (WINDOWPROP NC.LogoWindow 'WINDOWTITLESHADE BLACKSHADE)
    (WINDOWPROP NC.LogoWindow 'TITLE (CONCAT "Release " NC.ReleaseIdentifier 
                                            "                                  "
                                            (SUBSTRING NC.SystemDate 1 9)))
    (OPENW NC.LogoWindow)
    (BITBLT LOGOBM 0 0 NC.LogoWindow 0 0 (BITMAPWIDTH LOGOBM)
           (BITMAPHEIGHT LOGOBM)
           'INPUT
           'REPLACE)))

(NC.SetupScreen
  (LAMBDA NIL                                                (* fgh%: " 3-Jan-85 14:09")
          
          (* * Setup the standard NoteCards screen)

    (AND (BOUNDP 'LOGOW)
         (WINDOWP LOGOW)
         (CLOSEW LOGOW))
    (AND (BOUNDP 'NC.LogoWindow)
         (WINDOWP NC.LogoWindow)
         (CLOSEW NC.LogoWindow))
    (NC.MakeLogoWindow)
    (MOVEW NC.LogoWindow 5 605)
    (TTY.PROCESS T)
    (SETQ ExecWindow (WFROMDS (TTYDISPLAYSTREAM)))
    (WINDOWPROP ExecWindow 'TITLE NIL)
    (SHAPEW ExecWindow (CREATEREGION 608 650 267 66))
    (WINDOWPROP PROMPTWINDOW 'TITLE NIL)
    (SHAPEW PROMPTWINDOW (CREATEREGION 350 721 526 78))
    (CROCK (CREATEREGION 882 650 127 147))
    (CLEARW PROMPTWINDOW)
    (CLEARW ExecWindow)
    (LAFITE 'OFF)
    (AND (BOUNDP 'PROCESS.STATUS.WINDOW)
         (WINDOWP PROCESS.STATUS.WINDOW)
         (CLOSEW PROCESS.STATUS.WINDOW))))
)
(PUTPROPS NCDEMO COPYRIGHT ("Xerox Corporation" 1986 1987))
